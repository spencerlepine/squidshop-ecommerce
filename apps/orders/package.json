  "dependencies": {
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "eslint-plugin-import": "^2.26.0",
    "express": "4.18.2",
    "express-http-proxy": "^1.6.3",
    "jest": "29.1.2",
    "mysql2": "2.3.3",
    "nodemon": "^2.0.20",
    "sequelize": "6.24.0",
    "sequelize-cli": "^6.5.1",
    "sqlite3": "5.1.2",
    "supertest": "^6.3.1"
  },
  "devDependencies": {
    "@spencerlepine/eslint-config-node": "^2.0.0",
    "@spencerlepine/prettier-config": "2.0.1",
    "eslint": "8.25.0",
    "prettier": "^2.7.1"
  }
