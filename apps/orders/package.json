{
  "name": "squidshop-orders",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "npm run db:migrate && NODE_ENV=test jest --forceExit --detectOpenHandles",
    "lint": "eslint --fix --ext .js .",
    "db:migrate": "sequelize db:migrate",
