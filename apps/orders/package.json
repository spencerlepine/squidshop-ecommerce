  "dependencies": {
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "eslint-plugin-import": "^2.26.0",
    "express": "^4.18.1",
    "express-http-proxy": "^1.6.3",
    "jest": "29.0.3",
    "mysql2": "2.3.3",
    "nodemon": "^2.0.19",
    "sequelize": "6.21.6",
    "sequelize-cli": "^6.4.1",
    "sqlite3": "5.0.11",
    "supertest": "^6.2.4"
  },
  "devDependencies": {
    "@spencerlepine/eslint-config-node": "^2.0.0",
    "@spencerlepine/prettier-config": "2.0.1",
    "eslint": "8.23.1",
    "prettier": "^2.7.1"
  }
