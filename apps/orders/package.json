  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "eslint-plugin-import": "^2.27.5",
    "express": "4.18.2",
    "express-http-proxy": "^1.6.3",
    "jest": "29.5.0",
    "mysql2": "2.3.3",
    "nodemon": "^2.0.22",
    "sequelize": "6.30.0",
    "sequelize-cli": "6.6.0",
    "sqlite3": "5.1.6",
    "supertest": "6.3.3"
  },
  "devDependencies": {
    "@spencerlepine/eslint-config-node": "^2.0.0",
    "@spencerlepine/prettier-config": "2.0.1",
    "eslint": "8.37.0",
    "prettier": "^2.8.7"
  }
